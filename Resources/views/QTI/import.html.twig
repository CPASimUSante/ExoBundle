<div class="modal fade" id="modalqtiimport" role="dialog" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                </br>
            </div>
            <div class="modal-body">
                <h3 class="panel-title">{{'QTI import'|trans}}</h3>
                <form id="qtiImportForm" enctype="multipart/form-data" method="post"
                      action="{{ path('ujm_qti_import', { 'redirection': 0 }) }}">
                    <input id="qtifile" name="qtifile" type="file" required="true" accept="application/zip"/>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">{{ 'cancel'|trans({}, 'platform') }}</button>
                        <button type="submit" class="btn btn-primary" id="submit_picture" onclick="check_form();">
                            {{ 'ok'|trans({}, 'platform') }}
                        </button>
                    </div>
                </form>
           </div>
        </div>
    </div>
</div>
<script type="text/javascript">
    $('#modalqtiimport').modal('toggle', 'show');
    function check_form() {
    }
</script>